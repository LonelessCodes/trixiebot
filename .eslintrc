        //         "format": ["camelCase"],
        //         "leadingUnderscore": "allow",
        //         "filter": {
        //             "match": true,
        //             "regex": "\\w+Id"
        //         }
        //     },
        //     {
        //         "selector": "function",
        //         "format": ["camelCase"]
        //     },
        //     {
        //         "selector": "parameter",
        //         "format": ["snake_case"],
        //         "leadingUnderscore": "allow"
        //     },
        //     {
        //         "selector": "parameter",
        //         "format": ["camelCase"],
        //         "filter": {
        //             "match": true,
        //             "regex": "\\w+Id"
        //         }
        //     },
        //     {
        //         "selector": "property",
        //         "format": ["snake_case", "UPPER_CASE"],
        //         "leadingUnderscore": "allow"
        //     },
        //     {
        //         "selector": "property",
